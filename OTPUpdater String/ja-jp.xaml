<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib">
    <sys:String x:Key="Culture">ja-jp</sys:String>
    <sys:String x:Key="Welcome">Office Tool Plusのダウンロードへようこそ</sys:String>
    <sys:String x:Key="SavePath">保存パス:</sys:String>
    <sys:String x:Key="UpToDate">すべてのファイルは最新のものです!</sys:String>
    <sys:String x:Key="NewVersion">新しいバージョンがアップデート可能です、すぐにアップデートすることをお勧めします</sys:String>
    <sys:String x:Key="FileCount">合計 %Num% 個のファイルを更新する必要があります</sys:String>
    <sys:String x:Key="UpgradeNow">今すぐ更新</sys:String>
    <sys:String x:Key="Retry">再試行</sys:String>
    <sys:String x:Key="FixingApplication">必要なファイルが見つかりません、自己修復中……</sys:String>
    <sys:String x:Key="Checking">情報チェックを実行中…</sys:String>
    <sys:String x:Key="Downloading">更新中…</sys:String>
    <sys:String x:Key="Error">エラー</sys:String>
    <sys:String x:Key="DownloadError">ダウンロードエラー</sys:String>
    <sys:String x:Key="CheckHashError">ハッシュ値の検証に失敗</sys:String>
    <sys:String x:Key="UpgradeSuccessful">更新が完了</sys:String>
</ResourceDictionary> 
