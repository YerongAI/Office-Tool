<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib">
    <sys:String x:Key="Culture">ru-ru</sys:String>

    <sys:String x:Key="Welcome">Welcome to download Office Tool Plus</sys:String>
    <sys:String x:Key="SavePath">Save path:</sys:String>
    <sys:String x:Key="UpToDate">Вы видите актульные данные!</sys:String>
    <sys:String x:Key="NewVersion">Доступная новая версия.</sys:String>
    <sys:String x:Key="FileCount">Счёт файлов: %Num%</sys:String>
    <sys:String x:Key="UpgradeNow">Обновит</sys:String>
    <sys:String x:Key="Retry">Повтор</sys:String>
    <sys:String x:Key="FixingApplication">Файлов/файла нет, исправьте приложение.</sys:String>
    <sys:String x:Key="Checking">Проверка информации…</sys:String>
    <sys:String x:Key="Downloading">Загрузка файлов…</sys:String>
    <sys:String x:Key="Error">Ошибка</sys:String>
    <sys:String x:Key="DownloadError">Ошибка загрузки</sys:String>
    <sys:String x:Key="CheckHashError">Проверка контрольной суммы с ошибкой.</sys:String>
    <sys:String x:Key="UpgradeSuccessful">Обновлено успешно!</sys:String>
</ResourceDictionary>