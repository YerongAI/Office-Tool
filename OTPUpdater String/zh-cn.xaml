<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib">
    <sys:String x:Key="Culture">zh-cn</sys:String>

    <sys:String x:Key="Welcome">欢迎使用 Office Tool Plus 在线下载程序</sys:String>
    <sys:String x:Key="SavePath">保存路径:</sys:String>
    <sys:String x:Key="UpToDate">所有文件均是最新版本！</sys:String>
    <sys:String x:Key="NewVersion">已有新版本可更新，建议立即更新</sys:String>
    <sys:String x:Key="FileCount">总共需要更新 %Num% 个文件</sys:String>
    <sys:String x:Key="UpgradeNow">立即更新</sys:String>
    <sys:String x:Key="Retry">重试</sys:String>
    <sys:String x:Key="FixingApplication">程序所需文件缺失，正在执行修复程序</sys:String>
    <sys:String x:Key="Checking">正在检查信息…</sys:String>
    <sys:String x:Key="Downloading">正在下载更新…</sys:String>
    <sys:String x:Key="Error">错误</sys:String>
    <sys:String x:Key="DownloadError">下载错误</sys:String>
    <sys:String x:Key="CheckHashError">校验哈希值失败</sys:String>
    <sys:String x:Key="UpgradeSuccessful">更新完成</sys:String>
</ResourceDictionary>